<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find motoman_control)/config/sia5/sia5_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager"
		type="spawner" respawn="false"
		output="screen" ns="/sia5"
		args="joint_state_controller
			  joint_s_position_controller joint_l_position_controller
			  joint_e_position_controller joint_u_position_controller
			  joint_r_position_controller joint_b_position_controller
			  joint_t_position_controller "/>

  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
		respawn="false" output="screen">
    <remap from="/joint_states" to="/sia5/joint_states" />
  </node>

</launch>
